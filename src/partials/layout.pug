doctype html
html(lang='en')
	block vars
	head
		title #{title}
		block head
			include head
	body(class= bodyClass)
		block noscript
			include noscript

		block mixins
			include mixins

		block navigation
			include navigation

		.container-fluid.p-0
			.row
				.col-sm-4.col-12
					block sidebar
						include sidebar
				.col-sm-8.col-12
					block content

		block footer
			include footer

		block modals
			include modals

	block scripts
		include scripts
